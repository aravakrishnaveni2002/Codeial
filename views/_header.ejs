<header>
   

    <div id="logo">
        Codial
    </div>

    <div id="nav-header">
        <% if (locals.user) { %>
            <a class="links-header" href="/users/profile/<%= user.id%>">
                <%= user.name %>
            </a>

            <a class="links-header" href="/users/sign-out">
                Sign out
            </a>
        <% } else { %>    
            <a class="links-header" href="/users/sign-up">
                Sign Up
            </a>

            <a class="links-header" href="/users/sign-in">
                Sign In
            </a>
        <% } %>
    </div>

</header>
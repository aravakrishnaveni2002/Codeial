
    <link rel="stylesheet" href="/css/layout.css">

    <h1 id="title">Codial | <%= title %></h1>

    <div id="container">
        
        <%- include('_posts') -%>

        <% if(locals.user) { %>
            <section id="friends">

            <div id="heading">Friends</div>

            <% for(u of all_users) { %>
            
                <a href="/users/profile/<%= u.id%>" id="each-user"><%= u.name%></a>
            <% } %>

            </section>
        <% } %> 
            
                

               
            
        
    </div>

